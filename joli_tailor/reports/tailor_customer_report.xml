        <record id="paperformat_tailor_cutomer_receipt" model="report.paperformat">
            <field name="name">Customer Report</field>
            <field name="default" eval="True" />
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">25</field>
            <field name="margin_bottom">95</field>
            <field name="margin_left">0</field>
            <field name="margin_right">0</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">25</field>
            <field name="dpi">90</field>
        </record>

        <record id="action_report_tailor_customer_receipt" model="ir.actions.report">
            <field name="name">Tailor customer Report</field>
            <field name="model">tailor.map</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">joli_tailor.report_tailor_customer_receipt</field>
            <field name="report_file">joli_tailor.report_tailor_customer_receipt</field>
            <field name="binding_model_id" ref="model_tailor_map" />
            <field name="binding_type">report</field>
            <field name="print_report_name">'Tailor Receipt - %s' % (object.name or 'New').replace('/','')</field>
        </record>

        <record id="joli_tailor.action_report_tailor_customer_receipt" model="ir.actions.report">
            <field name="paperformat_id" ref="joli_tailor.paperformat_tailor_cutomer_receipt" />
        </record>

