        <template id="report_tailor_customer_page">
            <t t-call="joli_tailor.report_tailor_customer_receipt_header" />
            <t t-call="web.external_layout">
                <div class="page" style="font-family: 'Noto Serif Gujarati';">
                    <div class="row" style="padding: 3px;">
                        <div class="col-8 row">
                            <div class="col-1">
                                <p style="color: #FF4500; font-size: 1rem;">નામ :</p>
                            </div>
                            <div class="col-11">
                                <p style="color: #FF4500; font-size: 1rem; border-bottom: 1px solid #FF4500;">
                                    <span t-esc="o.partner_id.name" />
                                </p>
                            </div>
                        </div>
                        <div class="col-4 row">
                            <div class="col-2">
                                <p style="color: #FF4500; font-size: 1rem;">નંબર :</p>
                            </div>
                            <div class="col-10">
                                <p style="color: #FF4500; font-size: 1rem; border-bottom: 1px solid #FF4500;">
                                    <span t-esc="o.name" />
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="row" style="padding: 3px;">
                        <div class="col-6 row">
                            <div class="col-2">
                                <p style="color: #FF4500; font-size: 1rem;">મોં.નં. :</p>
                            </div>
                            <div class="col-10">
                                <p style="color: #FF4500; font-size: 1rem; border-bottom: 1px solid #FF4500;">
                                    <span t-esc="o.mobile1" />
                                </p>
                            </div>
                        </div>
                        <div class="col-6 row">
                            <div class="col-2">
                                <p style="color: #FF4500; font-size: 1rem;">તારીખ :</p>
                            </div>
                            <div class="col-10">
                                <p style="color: #FF4500; font-size: 1rem; border-bottom: 1px solid #FF4500;">
                                    <span t-esc="o.order_date" />
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="row" style="padding-top: 15px;">
                        <table style="color: #FF4500; font-size: 1rem;border : 0;">
                            <thead style="color: #FF4500; font-size: 1rem;">
                                <tr>
                                    <th width="4%" style="padding: 3px; font-size: 1rem;">No.</th>
                                    <th style="padding: 3px; font-size: 1rem;">Item</th>
                                    <th width="12%" style="padding: 3px; font-size: 1rem; text-align: center;">Quantity</th>
                                    <th width="8%" style="padding: 3px; font-size: 1rem; text-align: center;">Price</th>
                                    <th width="8%" style="padding: 3px; font-size: 1rem; text-align: center;">Total</th>
                                    <!-- <th width="10%" style="padding: 3px; font-size: 1.5rem;">&#2801;</th> -->
                                </tr>
                            </thead>
                            <t t-set="count" t-value="1" />
                            <tbody>
                                <tr t-if="o.is_pair" style="font-size: 1rem;">
                                    <td style="padding: 2px; text-align: center;">
                                        <t t-esc="count" />
                                        <t t-set="count" t-value="count + 1" />
                                    </td>
                                    <td style="padding: 2px; padding-left: 5px;">
                                        <span>Pair</span>
                                    </td>
                                    <td style="padding: 2px; text-align: center;">
                                        <span t-esc="o.pair_qty"/>
                                    </td>
                                    <td style="padding: 2px; text-align: right;">
                                        <span t-field="o.company_id.currency_id.symbol"/>
                                        <t t-set="charge" t-value="o.company_id.pair_shirt_charge + o.company_id.pair_pent_charge" />
                                        <span t-esc="charge" />
                                    </td>
                                    <td style="padding: 2px; text-align: right;">
                                        <span t-field="o.company_id.currency_id.symbol"/>
                                        <span t-esc="o.pair_charge" />
                                    </td>
                                </tr>
                                <tr t-foreach="o.shirt_map_ids" t-as="line" t-if='o.is_shirt' style="font-size: 1rem; color: #FF4500;">
                                    <td style="padding: 2px; text-align: center;">
                                        <t t-esc="count" />
                                        <t t-set="count" t-value="count + 1" />
                                    </td>
                                    <td style="padding: 2px; padding-left: 5px;">
                                        <span>Shirt</span>
                                    </td>
                                    <td style="padding: 2px; text-align: center;">
                                        <span t-esc="o.shirt_qty"/>
                                    </td>
                                    <td style="padding: 2px; text-align: right;">
                                        <span t-field="o.company_id.currency_id.symbol"/>
                                        <span t-esc="o.company_id.shirt_charge" />
                                    </td>
                                    <td style="padding: 2px; text-align: right;">
                                        <span t-field="o.company_id.currency_id.symbol"/>
                                        <span t-esc="o.shirt_charge" />
                                    </td>
                                </tr>
                                <tr t-foreach="o.pent_map_ids" t-as="line" t-if='o.is_pent' style="font-size: 1rem; color: #FF4500;">
